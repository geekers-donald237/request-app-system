 private fetchData(): void {
    const userId = this.utils.getUserIdFromLocalStorage();

    if (userId) {
      forkJoin([
        this.ueService.getRequestPatterns(userId),
        this.requestService.getRequestPatterns(),
      ]).subscribe(
        ([requestPatternsResponse, IStudentSchoolDataResponse]: [IRequestPatternsResponse, IStudentSchoolDataResponse]) => {
          // Log or handle the data as needed
          console.log('Request Patterns Response:', requestPatternsResponse);
          console.log('Student Data Response:', studentDataResponse);

          // Assign the data to class properties if needed
          this.requestPatternsData = requestPatternsResponse;
          this.studentData = studentDataResponse;

          // You can also call specific methods to handle each type of data
          // this.handleRequestPatternsResponse(requestPatternsResponse);
          // this.handleStudentDataResponse(studentDataResponse);
        },
        (error) => {
          console.error('Erreur lors de la récupération des données:', error);
        }
      );
    } else {
      console.error('ID utilisateur non trouvé dans le stockage local.');
    }
  }
